{
  "extends":         "./tsconfig.app.json",
  "include":         [
    "./cypress/support/**/*.ts",
    "./cypress/fixtures/**/*.ts",
    "./src/**/*",
    "env.d.ts"
  ],
  "exclude":         [],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "lib":             [],
    "types":           [
      "node",
      "jsdom"
    ],
    "paths":           {
      "@cypress/*": [
        "./cypress/*"
      ],
      "@/*":        [
        "./src/*"
      ]
    }
  }
}
